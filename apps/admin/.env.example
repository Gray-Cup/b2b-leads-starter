# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co

# Server-side key (use ONE of the following)
SUPABASE_SECRET_KEY=sb_secret_your_secret_key          # New format (preferred)
# SUPABASE_SERVICE_ROLE_KEY=your_service_role_key      # Legacy format (deprecated)

# Client-side configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# Client-side key (use ONE of the following)
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=sb_publishable_your_key  # New format (preferred)
# NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key                  # Legacy format (deprecated)

# Authentication
JWT_SECRET=openssl rand -hex 32

# Admin Credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_password_here
